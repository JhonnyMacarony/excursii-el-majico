<script>
    const alfabetBtn = document.getElementById("alfabet-btn")
    const classicBtn = document.getElementById("classic-btn")

    const alfabetContext = document.getElementById("alfabet")
    const normalContext = document.getElementById("normal")

    alfabetBtn.addEventListener("click", ()=>{
        alfabetContext.classList.remove("invisible")
        normalContext.classList.add("invisible")
        classicBtn.classList.add("alfabet-clicked")
        alfabetBtn.classList.remove("alfabet-clicked")
        alfabetBtn.classList.add("invisible")
        classicBtn.classList.remove("invisible")
        translateBtnUsa.classList.add("invisible")
        translateBtnRo.classList.add("invisible")
    })

    classicBtn.addEventListener("click", ()=>{
        alfabetContext.classList.add("invisible")
        normalContext.classList.remove("invisible")
        alfabetBtn.classList.add("alfabet-clicked")
        classicBtn.classList.remove("alfabet-clicked")
        classicBtn.classList.add("invisible")
        classicBtn.classList.add("invisible")
        alfabetBtn.classList.remove("invisible")
        alfabetBtn.classList.remove("alfabet-clicked")
        translateBtnUsa.classList.remove("invisible")
        translateBtnRo.classList.remove("invisible")
    })


    const translateBtnUsa = document.getElementById("translation-btn-usa")
    const translateBtnRo = document.getElementById("translation-btn-ro")
    const paragraphUsa = document.getElementById("paragraph-usa")
    const paragraphRo = document.getElementById("paragraph-ro")

    translateBtnUsa.addEventListener("click", ()=>{
        paragraphUsa.classList.remove("invisible")
        normalContext.classList.add("invisible")
        alfabetBtn.classList.add("invisible")
        classicBtn.classList.add("invisible")
    })

    translateBtnRo.addEventListener("click", ()=>{
        paragraphUsa.classList.add("invisible")
        normalContext.classList.remove("invisible")
        classicBtn.classList.add("invisible")
        alfabetBtn.classList.add("invisible")
    })

</script>
